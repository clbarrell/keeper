{
  "name": "Keeper",
  "version": "0.1.0",
  "description": "Keep an eye on your tab activity",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "title": "Tab Thief"
  },
  "icons": {
    "125": "logos/logo125.png",
    "250": "logos/logo250.png",
    "375": "logos/logo375.png"
  },
  "permissions": ["storage", "tabs", "activeTab", "notifications"],
  "content_scripts": []
}
